{
  "name": "sentry-testkit",
  "version": "5.0.5",
  "description": "Sentry Testkit enables Sentry to work natively in your application while running tests. The report is not really sent but rather logged locally into memory. In this way, the logged reports can be fetched for your own verification or any other purpose in your local development environment.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.json",
    "lint": "eslint --ext .js,.ts --fix",
    "prepare": "npm run-script build",
    "release": "standard-version"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zivl/sentry-testkit.git"
  },
  "keywords": [
    "raven-test-kit",
    "raven",
    "sentry",
    "sentry-io",
    "raven-js",
    "raven-node",
    "sentry-testkit",
    "error-reporting",
    "error-monitoring",
    "sentry-error-handler",
    "sentry-plugin",
    "sentry-client"
  ],
  "author": "Ziv Levy (levi.ziv@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zivl/sentry-testkit/issues"
  },
  "homepage": "https://zivl.github.io/sentry-testkit",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/preset-env": "^7.15.8",
    "@sentry/browser": "^7.5.0",
    "@sentry/node": "^7.5.0",
    "@sentry/tracing": "^7.5.0",
    "@sentry/types": "^7.5.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.0.0",
    "@types/nock": "^10.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "docsify-cli": "^4.4.3",
    "eslint": "^8.0.1",
    "eslint-plugin-prettier": "^3.4.1",
    "execa": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "lint-staged": "^10.5.4",
    "nock": "^10.0.6",
    "node-fetch": "^2.6.7",
    "prettier": "^1.19.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.0",
    "typescript": "^4.8.4",
    "wait-for-expect": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
